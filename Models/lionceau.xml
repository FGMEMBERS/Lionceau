<?xml version="1.0"?>

<PropertyList>

  <path>lionceau.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>fuselage</object-name>
    <object-name>entreesair</object-name>
    <object-name>interieur</object-name>
    <object-name>backpanel</object-name>
    <object-name>aile</object-name>
    <object-name>aile2</object-name>
    <object-name>aileronG</object-name>
    <object-name>aileronD</object-name>
    <object-name>voletG</object-name>
    <object-name>voletD</object-name>
    <object-name>trimprof</object-name>
    <object-name>profondeur</object-name>
    <object-name>direction</object-name>
    <object-name>roueA</object-name>
    <object-name>roueG</object-name>
    <object-name>roueD</object-name>
    <object-name>bol</object-name>
    <object-name>helice</object-name>
    <object-name>barreverriere</object-name>
    <object-name>dosverriere</object-name>
    <object-name>poigne</object-name>
    <object-name>siege</object-name>
    <object-name>siege2</object-name>
    <object-name>axeA</object-name>
    <object-name>axeG</object-name>
    <object-name>axeD</object-name>
    <object-name>axeA2</object-name>
    <object-name>axeG2</object-name>
    <object-name>axeD2</object-name>
    <object-name>cacheA</object-name>
    <object-name>cacheG</object-name>
    <object-name>cacheD</object-name>
    <object-name>pito</object-name>
    <object-name>antenne</object-name>
    <object-name>essieuG</object-name>
    <object-name>disqueG</object-name>
    <object-name>machoireG</object-name>
    <object-name>tuyauG</object-name>
    <object-name>tuyauG2</object-name>
    <object-name>essieuD</object-name>
    <object-name>disqueD</object-name>
    <object-name>machoireD</object-name>
    <object-name>tuyauD</object-name>
    <object-name>tuyauD2</object-name>
    <object-name>pocheG</object-name>
    <object-name>pocheD</object-name>
    <object-name>cacheH</object-name>
    <object-name>cacheB</object-name>
    <object-name>cableH</object-name>
    <object-name>cableB</object-name>
    <object-name>pilote</object-name>

    <!-- Objets transparents -->
    <object-name>verriere</object-name>
    <object-name>verriere1</object-name>
    <object-name>verriere2</object-name>
    <object-name>verriere3</object-name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>verriere</object-name>
    <object-name>verriere1</object-name>
    <object-name>verriere2</object-name>
    <object-name>verriere3</object-name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
  </animation>

  <!-- Une ombre pour OSG -->
  <model>
    <name>shadowOSG</name>
    <path>Aircraft/Lionceau/Models/shadow.xml</path>
  </model>

  <animation>
    <type>select</type>
    <object-name>shadowOSG</object-name>
    <condition>
      <equals>
        <property>/sim[0]/rendering/shadows-ai</property>
        <value>true</value>
      </equals>
    </condition>
  </animation>

  <!-- Planche de bord -->
  <!-- Principal -->
  <model>
    <path>Aircraft/Lionceau/Models/Panel/front-panel.xml</path>
    <offsets>
      <x-m> -1.543 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.028 </z-m>
      <pitch-deg> -15.488 </pitch-deg>
    </offsets>
  </model>

  <!-- Secondaire -->
  <model>
    <path>Aircraft/Lionceau/Models/Panel/between-seats-panel.xml</path>
    <offsets>
      <x-m> -1.271 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.355 </z-m>
    </offsets>
  </model>

  <!-- Pilote invisible depuis le cockpit -->
  <animation>
    <type>select</type>
    <object-name>pilote</object-name>
    <condition>
      <greater-than>
        <property>sim/current-view/view-number</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <name>HeliceComplete</name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
    <object-name>helice</object-name>
    <object-name>bol</object-name>
  </animation>

  <!-- L'hélice -->
  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdisc</object-name>
    <condition>
      <greater-than>
        <property>/engines/engine[0]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblur</object-name>
    <condition>
      <greater-than>
        <property>/engines/engine[0]/rpm</property>
        <value>600.000</value>
      </greater-than>
      <less-than>
        <property>/engines/engine[0]/rpm</property>
        <value>1500.001</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>helice</object-name>
    <condition>
      <less-than>
        <property>/engines/engine[0]/rpm</property>
        <value>1500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>HeliceComplete</object-name>
    <property>/engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -3.092 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.125 </z-m>
    </center>
    <axis>
      <x> -1.000 </x>
      <y>  0.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation> -->

  <!-- Gouverne de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>profondeur</object-name>
    <object-name>trimprof</object-name>
    <property>/controls/flight/elevator</property>
    <factor>11</factor>
    <center>
      <x-m> 3.108 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.021 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

 <!-- Gouverne de trim de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>trimprof</object-name>
    <property>/controls/flight/elevator-trim</property>
    <interpolation>
      <entry>
        <ind> -1.000 </ind>
        <dep> -5.000 </dep>
      </entry>
      <entry>
        <ind>  0.000 </ind>
        <dep>  0.000 </dep>
      </entry>
      <entry>
        <ind>  1.000 </ind>
        <dep>  5.000 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> 3.327 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.021 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>direction</object-name>
    <property>/controls/flight/rudder</property>
    <factor>20</factor>
    <axis>
      <x1-m>  2.774 </x1-m>
      <y1-m>  0.000 </y1-m>
      <z1-m>  0.104 </z1-m>
      <x2-m>  2.953 </x2-m>
      <y2-m>  0.000 </y2-m>
      <z2-m>  1.185 </z2-m>
    </axis>
  </animation>

  <!-- Animations des ailerons -->
  <animation>
    <type>rotate</type>
    <object-name>aileronG</object-name>
    <property>/surface-positions/left-aileron-pos-norm</property>
    <factor>15.0</factor>
    <axis>
      <x1-m> -0.523 </x1-m>
      <y1-m> -4.057 </y1-m>
      <z1-m> -0.321 </z1-m>
      <x2-m> -0.453 </x2-m>
      <y2-m> -2.903 </y2-m>
      <z2-m> -0.382 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>aileronD</object-name>
    <property>/surface-positions/right-aileron-pos-norm</property>
    <factor>15.0</factor>
    <axis>
      <x1-m> -0.453 </x1-m>
      <y1-m>  2.903 </y1-m>
      <z1-m> -0.382 </z1-m>
      <x2-m> -0.523 </x2-m>
      <y2-m>  4.057 </y2-m>
      <z2-m> -0.321 </z2-m>
    </axis>
  </animation>

  <!-- Volets -->
  <animation>
    <type>rotate</type>
    <object-name>voletG</object-name>
    <property>/surface-positions/flap-pos-norm</property>
    <factor>25.0</factor>
    <axis>
      <x1-m> -0.311 </x1-m>
      <y1-m> -2.308 </y1-m>
      <z1-m> -0.580 </z1-m>
      <x2-m> -0.240 </x2-m>
      <y2-m> -1.126 </y2-m>
      <z2-m> -0.642 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>voletD</object-name>
    <property>/surface-positions/flap-pos-norm</property>
    <factor>25.0</factor>
    <axis>
      <x1-m> -0.240 </x1-m>
      <y1-m>  1.126 </y1-m>
      <z1-m> -0.642 </z1-m>
      <x2-m> -0.311 </x2-m>
      <y2-m>  2.308 </y2-m>
      <z2-m> -0.580 </z2-m>
    </axis>
  </animation>

  <!-- Rotation de la roue avant en fonction de la gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>roueA</object-name>
    <object-name>cacheA</object-name>
    <object-name>axeA</object-name>
    <property>/controls/flight/rudder</property>
    <factor> 10.0 </factor>
    <center>
      <x-m> -2.210 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.047 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y>  0.000 </y>
      <z> -1.000 </z>
    </axis>
  </animation>

  <!-- Compression -->
  <animation>
    <type>translate</type>
    <object-name>roueA</object-name>
    <object-name>cacheA</object-name>
    <object-name>axeA2</object-name>
    <property>/gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry>
        <ind>  0.0 </ind>
        <dep> -0.1 </dep>
      </entry>
      <entry>
        <ind> 1.000 </ind>
        <dep> 0.150 </dep>
      </entry>
    </interpolation>
    <axis>
      <x>  0.02088  </x>
      <y> -9.9e-05  </y>
      <z>  0.096061 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>roueG</object-name>
    <object-name>axeG2</object-name>
    <object-name>cacheG</object-name>
    <object-name>essieuG</object-name>
    <object-name>disqueG</object-name>
    <object-name>machoireG</object-name>
    <object-name>tuyauG2</object-name>
    <property>/gear/gear[1]/compression-norm</property>
    <interpolation>
      <entry>
        <ind> 0.0 </ind>
        <dep> 0.0 </dep>
      </entry>
      <entry>
        <ind>  1.0 </ind>
        <dep> 10.0 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -0.639 </x-m>
      <y-m> -0.755 </y-m>
      <z-m> -0.737 </z-m>
    </center>
    <axis>
      <x> -1.000 </x>
      <y>  0.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>roueD</object-name>
    <object-name>axeD2</object-name>
    <object-name>cacheD</object-name>
    <object-name>essieuD</object-name>
    <object-name>disqueD</object-name>
    <object-name>machoireD</object-name>
    <object-name>tuyauD2</object-name>
    <property>/gear/gear[2]/compression-norm</property>
    <interpolation>
      <entry>
        <ind> 0.0 </ind>
        <dep> 0.0 </dep>
      </entry>
      <entry>
        <ind>  1.0 </ind>
        <dep> 10.0 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -0.639 </x-m>
      <y-m>  0.755 </y-m>
      <z-m> -0.737 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Rotation des roues lors du roulage -->
  <animation>
    <type>spin</type>
    <object-name>roueA</object-name>
    <property>/gear/gear[0]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -2.210 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.047 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueG</object-name>
    <object-name>essieuG</object-name>
    <object-name>disqueG</object-name>
    <property>/gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -0.562 </x-m>
      <y-m> -1.015 </y-m>
      <z-m> -1.047 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueD</object-name>
    <object-name>essieuD</object-name>
    <object-name>disqueD</object-name>
    <property>/gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -0.562 </x-m>
      <y-m>  1.015 </y-m>
      <z-m> -1.047 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <!-- Canopy -->
  <animation>
    <type>translate</type>
    <object-name>verriere2</object-name>
    <object-name>verriere3</object-name>
    <object-name>barreverriere</object-name>
    <object-name>dosverriere</object-name>
    <object-name>poigne</object-name>
    <property>/instrumentation/doors/crew/position-norm</property>
    <factor>1.1</factor>
    <axis>
      <x> 0.332287 </x>
      <y> 0.000000 </y>
      <z> 0.034530 </z>
    </axis>
  </animation>

</PropertyList>

