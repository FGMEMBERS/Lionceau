<?xml version="1.0"?>

<PropertyList>

 <path>OilGauge.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>PtrTipL</object-name>   
  <object-name>PtrTipR</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>

 <animation> 
  <type>rotate</type>
  <object-name>PtrL</object-name>
  <object-name>PtrTipL</object-name>
  <property>engines/engine/oil-temperature-degf</property>
  <interpolation>
   <entry><ind>104</ind><dep> 0</dep></entry>
   <entry><ind>140</ind><dep>16</dep></entry>
   <entry><ind>212</ind><dep>58</dep></entry>
   <entry><ind>266</ind><dep>89</dep></entry>
  </interpolation>
  <axis> 
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>-0.02</y-m>
   <z-m>0</z-m>  
  </center>
 </animation>         
 
 <animation> 
  <type>rotate</type>
  <object-name>PtrR</object-name>
  <object-name>PtrTipR</object-name>
  <property>engines/engine/oil-pressure-psi</property>
  <interpolation>
   <entry><ind>  0</ind><dep> 0</dep></entry>
   <entry><ind> 30</ind><dep>29</dep></entry>
   <entry><ind> 90</ind><dep>70</dep></entry>
   <entry><ind>120</ind><dep>84.5</dep></entry>
  </interpolation>
  <axis> 
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0.02</y-m>
   <z-m>0</z-m>  
  </center>
 </animation>         
 
</PropertyList>
