<?xml version="1.0"?>

<PropertyList>

 <path>EGT-OAT.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>PtrTipL</object-name>   
  <object-name>PtrTipR</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>

 <animation> 
  <type>rotate</type>
  <object-name>PtrL</object-name>
  <object-name>PtrTipL</object-name>
  <property>engines/engine/egt-degf</property>
  <interpolation>
   <entry><ind>1200</ind><dep> 0</dep></entry>
   <entry><ind>1750</ind><dep>85</dep></entry>
  </interpolation>
  <axis> 
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>-0.02</y-m>
   <z-m>0</z-m>  
  </center>
 </animation>         
 
 <animation> 
  <type>rotate</type>
  <object-name>PtrR</object-name>
  <object-name>PtrTipR</object-name>
  <property>environment/temperature-degc</property>
  <interpolation>
   <entry><ind>-20</ind><dep> 0</dep></entry>
   <entry><ind>  0</ind><dep>21</dep></entry>
   <entry><ind> 20</ind><dep>47</dep></entry>
   <entry><ind> 40</ind><dep>65</dep></entry>
   <entry><ind> 60</ind><dep>76.5</dep></entry>
  </interpolation>
  <axis> 
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0.02</y-m>
   <z-m>0</z-m>  
  </center>
 </animation>         
 
</PropertyList>
